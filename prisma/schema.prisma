generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Products {
  id        String   @id
  name      String
  price     Int
  status    String
  image     String
  createdAt DateTime @default(now())
  updatedAt DateTime
  ram       String?
  link      String?
}
